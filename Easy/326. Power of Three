class Solution {
public:
    bool isPowerOfThree(int n) {
        if(n>=0){
            int i=3;
            long long ans=1;
            while(n>ans)
                ans*=i;
            return ans==n;
        }
        return false;
    }
};


return (Math.log(n) / Math.log(3) + epsilon) % 1 <= 2 * epsilon;
public class Solution {
    public boolean isPowerOfThree(int n) {
        return n > 0 && 1162261467 % n == 0;
    }
}
