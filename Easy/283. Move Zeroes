class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int s=0,e=1;
        while(e!=nums.size())
        {
            if(nums[s]==0 && nums[e]!=0)
            {
                swap(nums[s],nums[e]);
                s++,e++;
            }
            else if(nums[s]==0 && nums[e]==0)
            {
                e++;
            }
            else
            {
                s++,e++;
            }
        }
    }
};
