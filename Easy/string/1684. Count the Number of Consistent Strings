//bruteforce pro max

class Solution {
    public int countConsistentStrings(String allowed, String[] words) {
        int count=0;
        for(int i=0;i<words.length;i++)
        {
            for(int j=0;j<allowed.length();j++)
            {
                words[i]=words[i].replaceAll((Character.toString(allowed.charAt(j))),"");
            }
        }
        for(int i=0;i<words.length;i++)
        {
            if(words[i].length()>0)
                continue;
            else
                count++;
        }
        return count;
    }
}
