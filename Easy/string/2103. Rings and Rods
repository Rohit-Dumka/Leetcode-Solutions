class Solution {
public:
    int countPoints(string rings) {
        unordered_map<int, unordered_set<char>>m;
        int ans=0;
        for(int i=1;i<rings.size();i+=2)
        {
           m[rings[i]-'0'].insert(rings[i-1]);
        }
        for(int i=0;i<10;i++)
        {
            if(m.find(i)!=m.end())
                if(m.at(i).size()==3)
                    ans++;
        }
        return ans;
    }
};
