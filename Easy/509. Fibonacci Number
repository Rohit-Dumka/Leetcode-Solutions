class Solution {
public:
    int fib(int n) {
        if(n==0) return 0;
        if(n==1) return 1;
        if(n==2) return 1;
        int sprev=0;
        int prev=1;
        int next=1;

        for(int i=3;i<=n;i++){
            sprev=prev;
            prev=next;
            next=sprev+prev;
        }

        return next;
    }
};
